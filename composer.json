    "keywords": ["laravel cart","shopping cart","easycart"],
    "require": {
        "php": "^5.6|^7.0|^8.0",
        "illuminate/support": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.* || ^8.0 || ^9.0",
        "orchestra/testbench": "3.4.*",
        "mockery/mockery":"0.9.*"
    },
    "autoload": {
        "psr-0": {
